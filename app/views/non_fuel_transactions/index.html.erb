<h1>Listing non_fuel_transactions</h1>

<table>
  <thead>
    <tr>
      <th>Transactions</th>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @non_fuel_transactions.each do |non_fuel_transaction| %>
      <tr>
        <td><%= non_fuel_transaction.transactions %></td>
        <td><%= non_fuel_transaction.product %></td>
        <td><%= link_to 'Show', non_fuel_transaction %></td>
        <td><%= link_to 'Edit', edit_non_fuel_transaction_path(non_fuel_transaction) %></td>
        <td><%= link_to 'Destroy', non_fuel_transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Non fuel transaction', new_non_fuel_transaction_path %>
